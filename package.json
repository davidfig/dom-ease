    "license": "MIT",
    "bugs": {
        "url": "https://github.com/davidfig/dom-ease/issues"
    },
    "homepage": "https://github.com/davidfig/dom-ease#readme",
    "dependencies": {
        "eventemitter3": "^3.0.0",
        "exists": "^1.0.1",
        "penner": "^0.1.3"
    },
    "devDependencies": {
        "@pixi/jsdoc-template": "^2.4.0",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "fork-me-github": "^1.0.3",
        "highlight.js": "^9.12.0",
        "jsdoc": "^3.5.5",
        "yy-fps": "^0.5.3",
        "yy-random": "^1.6.0"
    }
